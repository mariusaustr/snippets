# Bitbucket account 1
Host bitbucket.org
    HostName bitbucket.org
    User git
    IdentityFile ~/.ssh/id_rsa
    IdentitiesOnly yes


# Bitbucket account 2
# Important: change repository domain when cloning repo (git clone git@bitbucket.org.tc:vendor/repository.git)
 Host bitbucket.org.tc
     HostName bitbucket.org
     User git
     IdentityFile ~/.ssh/id_rsa_tc
     IdentitiesOnly yes

# Github account 1
Host github.com
    HostName github.com
    User git
    IdentityFile ~/.ssh/id_rsa
    IdentitiesOnly yes
